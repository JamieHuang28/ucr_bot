<launch>
  <!-- launch the ros_control driver with parameters -->
  <rosparam command="load" file="$(find ucr_gazebo_control)/config/front_left_drive_controller.yaml" />
  <node name="front_left_drive_controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" args="front_left_drive_controller" >
  </node>
  
  <include file="$(find ucr_steer_drive_control)/launch/front_left_wheel_follow_controller.launch">
    <arg name="pub_topic" value="front_left_drive_controller/command" />
  </include>
</launch>
